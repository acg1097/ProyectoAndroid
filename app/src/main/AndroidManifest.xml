<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alexandre.proyectoandroid">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_original"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_original"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".vistas.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".vistas.cotejar.CotejarActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".vistas.extras.Splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".vistas.mover.AnadirMovimientoActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".vistas.cotejar.Editar_mobiliario"
            android:screenOrientation="portrait" />
        <activity android:name=".vistas.extras.LoginActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".vistas.cotejar.Editar_carro"
            android:screenOrientation="portrait" />
        <activity android:name=".vistas.MainActivity_jefe"
            android:screenOrientation="portrait" />
        <activity android:name=".vistas.registrar.RegistrarActivo"
            android:screenOrientation="portrait" />
    </application>

</manifest>